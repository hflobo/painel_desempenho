<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Ano</th>
        <th scope="col">Período</th>
        <th scope="col">Valor</th>
        <th scope="col">Indicador</th>
        <th scope="col">ID do valor</th>
      </tr>
    </thead>
    <tbody>
      <% @values.each do |value| %>
      <tr>
        <th scope="row"><%= value.ano %></th>
        <td><%= value.periodo %></td>
        <td><%= value.valor %></td>
        <td><%= value.indicator.nome %></td>
        <td><%= value.id %></td>
        <td><a><%= link_to "Ver", value_path(value), class: "a-primary" %><i class="fas fa-search-plus"></i></a></td>
        <td><a><%= link_to "Editar", edit_value_path(value), class: "a-primary" %><i class="fas fa-edit"></i></a></td>
        <td><a><%= link_to "Delete #{value.id}", value_path(value), method: :delete, data: { confirm: "Você tem certeza?" }, class: "a-primary" %><i class="fas fa-trash-alt"></i></a></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>